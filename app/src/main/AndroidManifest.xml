<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.timeron.timeronwallet">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- <activity android:name=".CalculatorActivity"> -->
        <activity android:name=".WalletMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CalculatorActivity">
            <intent-filter>
                <action android:name="calculator_activity" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CalendarActivity" />
        <activity
            android:name=".RecordListActivity" />
        <activity
            android:name=".actitityModule.RecordRowActivity" />
        <activity
            android:name=".actitityModule.BillRowActivity" />
        <activity
            android:name=".BillActivity"
            android:label="@string/title_activity_bill"/>
    </application>

</manifest>